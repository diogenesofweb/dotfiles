{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "css",
          "scss",
          "sass"
        ],
        "path": "./snippets/css.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "vue",
          "svelte"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": "svelte",
        "path": "./snippets/svelte.json"
      },
      {
        "language": "go",
        "path": "./snippets/go.json"
      }
    ]
  }
}
